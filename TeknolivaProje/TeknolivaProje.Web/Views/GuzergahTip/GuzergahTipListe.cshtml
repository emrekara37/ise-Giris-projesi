@model IEnumerable<TeknolivaProje.Data.Models.ROUTE_TYPE>
<div class="col-lg-6">
  <h2>Güzergah Tip Listesi</h2>
  <hr />
  <div class="table-responsive">
    <table class="table table-bordered table-hover" id="guzergahTipleriYazdir">
      <thead>
      <tr>
        <th>Güzergah Tip</th>
        <th class="noPrint">İşlemler</th>

      </tr>
      </thead>
      <tbody >
 

      @foreach (var item in Model)
      {


          
        <tr>
             
          <td>
            <input type="text" value="@item.ROUTE_TYPE_NAME" data-id="@item.ROUTE_TYPE_ID" disabled="disabled" class="form-control txtGuzergahTip" />

          </td>

          <td class="noPrint">
            <button data-id="@item.ROUTE_TYPE_ID" class="btn btn-warning btnGuzergahTipDuzenle">Düzenle</button>
             
            <button data-id="@item.ROUTE_TYPE_ID" class="btn btn-danger btnGuzergahTipSil">Sil</button>
          </td>

        </tr>


      }
   
      </tbody>
    </table>
  </div>
</div>
