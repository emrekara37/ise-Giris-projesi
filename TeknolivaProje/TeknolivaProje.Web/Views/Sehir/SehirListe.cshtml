@model IEnumerable<TeknolivaProje.Data.Models.CITY>
<div class="col-lg-6">
  <h2>Şehir Listesi</h2>
  <hr />
  <div class="table-responsive">
    <table class="table table-bordered table-hover" id="illeriYazdir">
      <thead>
      <tr>
        <th>Şehir İsmi</th>
        <th class="noPrint">İşlemler</th>

      </tr>
      </thead>
      <tbody >
 

      @foreach (var item in Model)
      {


          
        <tr>
             
          <td>
            <input type="text" value="@item.CITY_NAME" data-id="@item.CITY_ID" disabled="disabled" class="form-control txtSehirAdi" />

          </td>

          <td class="noPrint">
            <button data-id="@item.CITY_ID" class="btn btn-warning btnSehirDuzenle">Düzenle</button>
             
            <button data-id="@item.CITY_ID" class="btn btn-danger btnSehirSil">Sil</button>
          </td>

        </tr>


      }
   
      </tbody>
    </table>
  </div>
</div>
